{
  "name": "prime",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "use:dev": "cp firebase.json f.json",
    "use:test": "cp package.json f.json",
    "c:dev": "vite --mode dev",
    "c:build": "tsc && vite build",
    "c:preview": "vite preview",
    "c:host": "firebase emulators:start --only hosting",
    "s:shell": "firebase functions:shell",
    "s:dev": "nodemon",
    "s:serve": "firebase emulators:start --only functions",
    "s:deploy": "firebase deploy --only functions",
    "s:logs": "firebase functions:log",
    "test:unit:client": "jest --selectProjects 'unit:client'",
    "test:unit:server": "jest --selectProjects 'unit:server'",
    "test:unit": "npm run test:unit:client && npm run test:unit:server",
    "prepare": "husky",
    "lint": "eslint \"client/**/*\" \"server/**/*\" ",
    "prettier": "prettier --write \"client/**/*\" \"server/**/*\" ",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.19.2",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@eslint/js": "^9.1.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.3",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "firebase-functions-test": "^3.1.0",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.7.0",
    "vite": "^5.2.0"
  },
  "overrides": {
    "protobufjs": "^7.2.5"
  }
}
